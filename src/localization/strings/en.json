  "7tv": {
    "command": {
      "name": "7tv",
      "description": "7TV related commands"
    },
    "setup": {
      "command": {
        "name": "setup",
        "description": "Setup 7TV emotes for your server"
      }
    },
    "emote": {
      "command": {
        "name": "emote",
        "description": "Manage 7TV emotes for your server"
      },
